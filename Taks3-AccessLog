#extrair o recurso acessado pelo usu√°rio no log de um site e gravar em uma lista, depois por cada item da lista em 1 linha em um arquivo .txt
arquivo = open("access.log", encoding = "utf-8", errors = "ignore")
log = arquivo.read()
arquivo.close()

listarecursos = []
recursoacessado = ""
fasedaanalise = 1

for varrelistarecursos in log:
    if fasedaanalise == 1:
        if varrelistarecursos != "]":
            fasedaanalise = 1
        else:
            fasedaanalise = 2
    elif fasedaanalise == 2:
        if varrelistarecursos != " ":
            fasedaanalise = 2
        else:
            fasedaanalise = 3
    elif fasedaanalise == 3:
        if varrelistarecursos!= " ":
            fasedaanalise = 3
        else:
            fasedaanalise = 4
    elif fasedaanalise == 4:
        if varrelistarecursos != " ":
            recursoacessado = recursoacessado + varrelistarecursos
            fasedaanalise = 4
        else:
            listarecursos.append(recursoacessado)
            recursoacessado = ""
            fasedaanalise = 1
print(listarecursos)
arq = open("recursos.txt", "w")

for dado in listarecursos:
    arq.write(dado + "\n")

arq.close()
